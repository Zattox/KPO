{
  "Logging": {
    "LogLevel": {
      "Default": "${LOGGING_LOGLEVEL_DEFAULT}",
      "Microsoft.AspNetCore": "${LOGGING_LOGLEVEL_MICROSOFT_ASPNETCORE}"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_DATABASE};Username=${DB_USERNAME};Password=${DB_PASSWORD}"
  },
  "StorageDirectory": "${STORAGE_DIRECTORY}"
}